<!DOCTYPE html>
<html lang="en" ag-app>
<head>
  <title>Dashbord</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/7.0.6/sweetalert2.all.js"></script>
  
  <script type="text/javascript">
    function disableact(tex , y, z) {
    document.getElementById(y+z).onchange = function() {
    document.getElementById(tex+z).disabled = !this.checked;
    };
  }
  </script>

  <style>
      body {
        width: 1px;
        min-width: 100%;
        *width: 100%;
        background-color: #ECEFF1;
      }
      input[type=text] {
          width: 130px;
          box-sizing: border-box;
          border: 2px solid #ccc;
          border-radius: 4px;
          font-size: 16px;
          background-color: white;
          background-image: url('asset/image/searching.png');
          background-position: 10px 10px; 
          background-repeat: no-repeat;
          padding: 12px 20px 12px 40px;
          -webkit-transition: width 0.4s ease-in-out;
          transition: width 0.4s ease-in-out;
      }

      input[type=text]:focus {
          width: 100%;
      }
    </style>
</head>
<body ">
      <div class="bs-example">
          <%- include header.ejs %>
      </div>

      </br>
      </br>

<!--customer deteis and 1st grid--------------------> 


    <div class="row-md-*">
     
<!--customer details and 1st grid end point--------------------> 

   <!-- add, post details and 2nd grid--------------------> 
        <!-- Main content -->
    <section class="content">

    <div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-10">
      <h3>Add New Product</h3>
    </div>
    </div>

    <div class="row">
        
        <!-- /.col -->
    <div class="col-md-2">
    </div>
		<div class="col-md-3">

      <div class="box box-primary">
            <div class="box-body ">
            
              <img align="left" class="profile-user-img img-responsive " src="/cssFiles/product_pic.png" style="" >

            </div>
            <!-- /.box-body -->
       </div>
          <!-- /.box -->


      <form action="/addnewproductfull" method="post" encType="multipart/form-data">
        <div class="form-group">
          <label>Product Name & serial</label>
          <input type="text" class="form-control" placeholder="Product name" name="name">
          <br/>
          <input type="text" class="form-control"  placeholder="Serial Num" name="serial">
        </div>
        <div class="form-group">
          <label>Description</label>
          <br/>
          <textarea class="form-control"  placeholder="Product Description" name="des"></textarea>
        </div>
        <div class="form-group">
          <label>Product Price</label>
          <input type="text" class="form-control"  placeholder="Cost" name="cost">
          <br/>
          <input type="text" class="form-control"  placeholder="Retail Price" name="retailprice">
          <br/>
          <input type="text" class="form-control"  placeholder="Retail Credit" name="retailcredit">
        </div>
        
		</div>

    <div class="col-md-3">

        <div class="form-group">
          <label>Store</label>
          <select name="store" class="form-control">
              <%      
                if(store.length){ 
                for(var i = 0;i < store.length;i++) { 
              %>
              <option value="<%=store[i].name%>" ><%=store[i].name%></option>
              <%      
                }
              } 
              %>
        </select>
        </div>
        <div class="form-group">
          <label>Category</label>
          <select name="category" class="form-control">
              <%      
                if(category.length){ 
                for(var i = 0;i < category.length;i++) { 
              %>
              <option value="<%=category[i].name%>"><%=category[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        <div class="form-group">
          <label>Generic</label>
          <select name="generic" class="form-control">
              <%      
                if(generic.length){ 
                for(var i = 0;i < generic.length;i++) { 
              %>
              <option value="<%=generic[i].name%>"><%=generic[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        <div class="form-group">
          <label>Brand</label>
          <select name="brand" class="form-control">
              <%      
                if(brand.length){ 
                for(var i = 0;i < brand.length;i++) { 
              %>
              <option value="<%=brand[i].name%>"><%=brand[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        <div class="form-group">
          <label>Make</label>
          <select name="make" class="form-control">
              <%      
                if(make.length){ 
                for(var i = 0;i < make.length;i++) { 
              %>
              <option value="<%=make[i].name%>"><%=make[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        <div class="form-group">
          <label>Model</label>
          <select name="model" class="form-control">
              <%      
                if(model.length){ 
                for(var i = 0;i < model.length;i++) { 
              %>
              <option value="<%=model[i].name%>"><%=model[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        <div class="form-group">
          <label>Unit</label>
          <select name="unit" class="form-control">
          <datalist>
              <%      
                if(unit.length){ 
                for(var i = 0;i < unit.length;i++) { 
              %>
              <option value="<%=unit[i].name%>"><%=unit[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        <div class="form-group">
          <label>Supplier</label>
          <select name="supplier" class="form-control">
          <datalist >
              <%      
                if(supplier.length){ 
                for(var i = 0;i < supplier.length;i++) { 
              %>
              <option value="<%=supplier[i].name%>"><%=supplier[i].name%></option>
              <%      
                }
              } 
              %>
          </select>
        </div>
        

          
    </div>

    <div class="col-md-3">
        <div class="form-group">
          <label>Warenty Detils</label>
          <input type="text" class="form-control" placeholder="Warenty No" name="warentyno">
          <br/>
          From:
          <input type="date" class="form-control" name="warentyfrom">
          <br/>
          To:
          <input type="date" class="form-control" name="warentyto">
        </div>
        
        <div class="form-group">
          <button type="submit" class="btn btn-primary mb-2">Add to Database</button>
        </div>
        </form>
    </div>
		
        <!-- /.col -->
    </div>
      <!-- /.row -->

    </section>
    <!-- /.content -->


  
   
   
</div>






</div>
</div>


    <script>
      // execute/clear BS loaders for docs
      $(function(){while(window.BS&&window.BS.loader&&window.BS.loader.length){(window.BS.loader.pop())()}})
    </script>
 
 


</body>
</html>
